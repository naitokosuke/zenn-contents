{
  "files.exclude": {
    "**/node_modules/": true
  },
  "emmet.showSuggestionsAsSnippets": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "prettier.enable": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  }
}
